<!-- Footer Section End -->

        <script>
    const discounted = 0;
    const amount = 12;
    const amount_discounted = 12;
    const url = 'https://mijn.kaashosting.nl/cart.php?a=add&pid=98&skipconfig=1&promocode=';

    $('.billingcycle').children().click(function() {

        $('.billingcycle').children().each(function() {
            $(this).find('.content').html('<i class="mt-20 mb-20 zmdi zmdi-circle-o zmdi-hc-2x"></i>');
        });

        switch ($(this).attr('data-billing-type')) {
            case 'monthly':
                $('[data-billing-type=monthly]').find('.content').html('<i class="mt-20 mb-20 zmdi zmdi-check-circle zmdi-hc-2x theme-color-2"></i>');
                updatePrice(1);
                $('#order_button').prop('href', url + '&billingcycle=monthly');
                break;
            case 'quarterly':
                $('[data-billing-type=quarterly]').find('.content').html('<i class="mt-20 mb-20 zmdi zmdi-check-circle zmdi-hc-2x theme-color-2"></i>');
                updatePrice(0.98);
                $('#order_button').prop('href', url + '&billingcycle=quarterly');
                break;
            case 'semiannually':
                $('[data-billing-type=semiannually]').find('.content').html('<i class="mt-20 mb-20 zmdi zmdi-check-circle zmdi-hc-2x theme-color-2"></i>');
                updatePrice(0.96);
                $('#order_button').prop('href', url + '&billingcycle=semiannually');
                break;
            case 'annually':
                $('[data-billing-type=annually]').find('.content').html('<i class="mt-20 mb-20 zmdi zmdi-check-circle zmdi-hc-2x theme-color-2"></i>');
                updatePrice(0.92);
                $('#order_button').prop('href', url + '&billingcycle=annually');
                break;
            default:

        }
    });

    function parsePrice(amount, discount) {
        let price = (amount * discount).toFixed(2).replace(".", ",")

        return price;
    }

    function updatePrice(discount) {
        $('#price').html(parsePrice(amount, discount));
        $('#price_discounted').html(parsePrice(amount_discounted, discount));
    }
    </script>

